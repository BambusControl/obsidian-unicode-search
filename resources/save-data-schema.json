{
    "$id": "https://raw.githubusercontent.com/BambusControl/obsidian-unicode-search/0.5.0-NEXT/resources/save-data-schema.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "BlockFilter": {
            "allOf": [
                {
                    "$ref": "https://raw.githubusercontent.com/BambusControl/obsidian-unicode-search/0.5.0-NEXT/resources/save-data-schema.json#/definitions/CodepointInterval"
                },
                {
                    "$ref": "https://raw.githubusercontent.com/BambusControl/obsidian-unicode-search/0.5.0-NEXT/resources/save-data-schema.json#/definitions/InclusionFlag"
                }
            ]
        },
        "CategoryFilter": {
            "properties": {
                "abbreviation": {
                    "$ref": "https://raw.githubusercontent.com/BambusControl/obsidian-unicode-search/0.5.0-NEXT/resources/save-data-schema.json#/definitions/CharacterCategoryType"
                },
                "included": {
                    "type": "boolean"
                },
                "name": {
                    "type": "string"
                }
            },
            "required": [
                "abbreviation",
                "included",
                "name"
            ],
            "type": "object"
        },
        "CategoryGroupFilter": {
            "properties": {
                "abbreviation": {
                    "$ref": "https://raw.githubusercontent.com/BambusControl/obsidian-unicode-search/0.5.0-NEXT/resources/save-data-schema.json#/definitions/CharacterCategoryGroupType"
                },
                "categories": {
                    "items": {
                        "$ref": "https://raw.githubusercontent.com/BambusControl/obsidian-unicode-search/0.5.0-NEXT/resources/save-data-schema.json#/definitions/CategoryFilter"
                    },
                    "type": "array"
                },
                "name": {
                    "type": "string"
                }
            },
            "required": [
                "abbreviation",
                "categories",
                "name"
            ],
            "type": "object"
        },
        "CharacterCategoryGroupType": {
            "enum": [
                "C",
                "L",
                "M",
                "N",
                "P",
                "S",
                "Z"
            ],
            "type": "string"
        },
        "CharacterCategoryType": {
            "enum": [
                "Cc",
                "Cf",
                "Cn",
                "Co",
                "Cs",
                "Ll",
                "Lm",
                "Lo",
                "Lt",
                "Lu",
                "Mc",
                "Me",
                "Mn",
                "Nd",
                "Nl",
                "No",
                "Pc",
                "Pd",
                "Pe",
                "Pf",
                "Pi",
                "Po",
                "Ps",
                "Sc",
                "Sk",
                "Sm",
                "So",
                "Zl",
                "Zp",
                "Zs"
            ],
            "type": "string"
        },
        "CodepointAttribute": {
            "properties": {
                "category": {
                    "type": "string"
                },
                "name": {
                    "description": "Unicode provided description of the character",
                    "type": "string"
                }
            },
            "required": [
                "category",
                "name"
            ],
            "type": "object"
        },
        "CodepointInterval": {
            "description": "Represents a closed interval/range of Unicode Code Points",
            "properties": {
                "end": {
                    "type": "number"
                },
                "start": {
                    "type": "number"
                }
            },
            "required": [
                "end",
                "start"
            ],
            "type": "object"
        },
        "CodepointKey": {
            "properties": {
                "codepoint": {
                    "description": "A single character defined by a Unicode code point",
                    "maxLength": 1,
                    "minLength": 1,
                    "type": "string"
                }
            },
            "required": [
                "codepoint"
            ],
            "type": "object"
        },
        "CodepointUsage": {
            "allOf": [
                {
                    "$ref": "https://raw.githubusercontent.com/BambusControl/obsidian-unicode-search/0.5.0-NEXT/resources/save-data-schema.json#/definitions/CodepointKey"
                },
                {
                    "$ref": "https://raw.githubusercontent.com/BambusControl/obsidian-unicode-search/0.5.0-NEXT/resources/save-data-schema.json#/definitions/UsageInfo"
                }
            ]
        },
        "FilterData": {
            "properties": {
                "categoryGroups": {
                    "items": {
                        "$ref": "https://raw.githubusercontent.com/BambusControl/obsidian-unicode-search/0.5.0-NEXT/resources/save-data-schema.json#/definitions/CategoryGroupFilter"
                    },
                    "type": "array"
                },
                "planes": {
                    "items": {
                        "$ref": "https://raw.githubusercontent.com/BambusControl/obsidian-unicode-search/0.5.0-NEXT/resources/save-data-schema.json#/definitions/PlaneFilter"
                    },
                    "type": "array"
                }
            },
            "required": [
                "categoryGroups",
                "planes"
            ],
            "type": "object"
        },
        "InclusionFlag": {
            "properties": {
                "included": {
                    "type": "boolean"
                }
            },
            "required": [
                "included"
            ],
            "type": "object"
        },
        "PlaneFilter": {
            "properties": {
                "blocks": {
                    "items": {
                        "$ref": "https://raw.githubusercontent.com/BambusControl/obsidian-unicode-search/0.5.0-NEXT/resources/save-data-schema.json#/definitions/BlockFilter"
                    },
                    "type": "array"
                },
                "end": {
                    "type": "number"
                },
                "start": {
                    "type": "number"
                }
            },
            "required": [
                "blocks",
                "end",
                "start"
            ],
            "type": "object"
        },
        "SaveDataVersion": {
            "description": "Version of the save data schema.\n\nMust comply with RegEx:\n```^[0-9]+\\\\.[0-9]+\\\\.[0-9]+(?:-[A-Z]+)?$```",
            "enum": [
                "0.4.0",
                "0.5.0",
                "0.6.0-NEXT"
            ],
            "type": "string"
        },
        "SettingsData": {
            "properties": {
                "filter": {
                    "$ref": "https://raw.githubusercontent.com/BambusControl/obsidian-unicode-search/0.5.0-NEXT/resources/save-data-schema.json#/definitions/FilterData"
                },
                "initialized": {
                    "type": "boolean"
                },
                "modified": {
                    "type": "boolean"
                }
            },
            "required": [
                "filter",
                "initialized",
                "modified"
            ],
            "type": "object"
        },
        "UnicodeCodepoint": {
            "allOf": [
                {
                    "$ref": "https://raw.githubusercontent.com/BambusControl/obsidian-unicode-search/0.5.0-NEXT/resources/save-data-schema.json#/definitions/CodepointKey"
                },
                {
                    "$ref": "https://raw.githubusercontent.com/BambusControl/obsidian-unicode-search/0.5.0-NEXT/resources/save-data-schema.json#/definitions/CodepointAttribute"
                }
            ]
        },
        "UnicodeData": {
            "properties": {
                "codepoints": {
                    "items": {
                        "$ref": "https://raw.githubusercontent.com/BambusControl/obsidian-unicode-search/0.5.0-NEXT/resources/save-data-schema.json#/definitions/UnicodeCodepoint"
                    },
                    "type": "array"
                },
                "initialized": {
                    "type": "boolean"
                }
            },
            "required": [
                "codepoints",
                "initialized"
            ],
            "type": "object"
        },
        "UsageData": {
            "properties": {
                "codepoints": {
                    "items": {
                        "$ref": "https://raw.githubusercontent.com/BambusControl/obsidian-unicode-search/0.5.0-NEXT/resources/save-data-schema.json#/definitions/CodepointUsage"
                    },
                    "type": "array"
                },
                "initialized": {
                    "type": "boolean"
                }
            },
            "required": [
                "codepoints",
                "initialized"
            ],
            "type": "object"
        },
        "UsageInfo": {
            "properties": {
                "firstUsed": {
                    "type": "string"
                },
                "lastUsed": {
                    "type": "string"
                },
                "useCount": {
                    "type": "number"
                }
            },
            "required": [
                "firstUsed",
                "lastUsed",
                "useCount"
            ],
            "type": "object"
        }
    },
    "properties": {
        "initialized": {
            "type": "boolean"
        },
        "settings": {
            "$ref": "https://raw.githubusercontent.com/BambusControl/obsidian-unicode-search/0.5.0-NEXT/resources/save-data-schema.json#/definitions/SettingsData"
        },
        "unicode": {
            "$ref": "https://raw.githubusercontent.com/BambusControl/obsidian-unicode-search/0.5.0-NEXT/resources/save-data-schema.json#/definitions/UnicodeData"
        },
        "usage": {
            "$ref": "https://raw.githubusercontent.com/BambusControl/obsidian-unicode-search/0.5.0-NEXT/resources/save-data-schema.json#/definitions/UsageData"
        },
        "version": {
            "$ref": "https://raw.githubusercontent.com/BambusControl/obsidian-unicode-search/0.5.0-NEXT/resources/save-data-schema.json#/definitions/SaveDataVersion"
        }
    },
    "required": [
        "initialized",
        "settings",
        "unicode",
        "usage",
        "version"
    ],
    "type": "object"
}

